{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/styles.css",
      "dest": "public/styles.css"
    },
    {
      "src": "/app.js",
      "dest": "public/app.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}
